<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Register regression tests">

    <test name="allow_to_register">

        <parameter name="browser" value="chrome"></parameter>

        <classes>
            <class name="registerTest">
                <methods>
                    <include name="allow_to_register"></include>
                </methods>
            </class>
        </classes>


    </test>

    <test name="allow_to_search_Product">

        <parameter name="browser" value="chrome"></parameter>


        <classes>
            <class name="searchProductDataTest">
                <methods>
                    <include name="allow_to_search_Product"></include>
                </methods>
            </class>
        </classes>


    </test>

    <test name="allow_to_currencies">

        <parameter name="browser" value="chrome"></parameter>


        <classes>
            <class name="currenciesTest">
                <methods>
                    <include name="allow_to_currencies"></include>
                </methods>
            </class>
        </classes>


    </test>

</suite>

